{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Application": {
    "MasterKey": "UNUSED_BUT_REQUIRED_FOR_UNIFY_ENCRYPTION"
  },
  "Unify": {
    "Uploads": {
      "BaseUrl": "http://localhost:5289",
      "IncompleteExpiration": 120,
      "CleanupInterval": 60,
      "Zones": {
        "MyUploadZone": {
          "Authorise": false,
          "Accepted": "xlsx,pdf",
          "MaxFiles": 3,
          "MinFiles": 2,
          "MaxSize": 200000
        }
      }
    }
  }
}
